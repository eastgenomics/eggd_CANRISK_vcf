{
    "name": "eggd_CANRISK_vcf",
    "title": "eggd_CANRISK_vcf",
    "summary": "eggd_CANRISK_vcf",
    "dxapi": "1.0.0",
    "version": "1.0.0",
    "authorizedUsers": [
        "org-emee_1"
      ],
      "developers":[
        "org-emee_1"
      ],
    "inputSpec": [
      {
        "name": "sample_vcf",
        "class": "file",
        "optional": false,
        "patterns": [
          "*.vcf.gz"
        ],
        "help": "VCF output of Sentieon DNASEQ (FASTQ>VCF). Must be contain genotypes for all positions specified in the PRS file."
      },
      {
        "name": "segments_vcf",
        "class": "file",
        "optional": true,
        "patterns": [
          "*segments.vcf"
        ],
        "help": "GATK gCNV segments vcf"
      },
      {
        "name": "PRS_variants",
        "class": "file",
        "optional": false,
        "patterns": [
          "*.prs"
        ],
        "help": "Comma separated PRS targets from Canrisk site. Must contain at least CHROM, POS, REF, & ALT fields."
      }
    ],
    "outputSpec": [
      {
        "name": "canrisk_PRS",
        "class": "file",
        "patterns": [
          "*"
        ],
        "help": ""
      },
      {
        "name": "cnv_check",
        "class": "file",
        "patterns": [
          "*"
        ],
        "help": ""
      },
      {
        "name": "coverage_check",
        "class": "file",
        "patterns": [
          "*"
        ],
        "help": ""
      }
    ],
    "runSpec": {
      "timeoutPolicy": {
        "*": {
          "hours": 2
        }
      },
      "interpreter": "bash",
      "file": "src/eggd_CANRISK_vcf.sh",
      "distribution": "Ubuntu",
      "release": "20.04",
      "version": "0", 
      "assetDepends": [
        {
          "name": "bedtools",
          "project": "project-Fkb6Gkj433GVVvj73J7x8KbV",
          "folder": "/app_assets/bedtools/bedtools_v2.30.0",
          "version": "2.30.0"
        },
        {
          "name": "htslib",
          "project": "project-Fkb6Gkj433GVVvj73J7x8KbV",
          "folder": "/app_assets/htslib/htslib_v1.15.0",
          "version": "1.15.0"
        }
      ]
    },
    "access": {
      "project": "CONTRIBUTE"
    },
    "regionalOptions": {
      "aws:eu-central-1": {
        "systemRequirements": {
          "*": {
            "instanceType": "mem1_ssd1_v2_x2"
          }
        }
      }
    }
  }
  